<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Quotes</title>
</head>

<body>
    <h1>All Quotes</h1>
    @{
        @foreach (var quote in ViewBag.Quotes)
        {
            <h3>"@quote["quote"]"</h3>
            <p>@quote["name"]</p>
            
        }
    }
</body>

</html>